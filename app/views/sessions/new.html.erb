<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>
	<h1>Log In</h1>
		<%= form_for(:session, :html => {role: "form"}, url: login_path) do |i| %>
		<div>
			<%= i.label :email %>
			<%= i.email_field :email, placeholder: "Enter Email" %>
		</div>

		<div>
			<%= i.label :password %>
			<%= i.password_field :password, autocomplete: "off" %>
		</div>

		<p>
			<%= i.submit "log in" %>
		</p>

		<% end %>
</body>
</html>